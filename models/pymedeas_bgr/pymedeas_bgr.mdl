Historic water by type intensities by sector[scenarios,sectors,water]=
	IF THEN ELSE( Time<2009, ZIDZ( Historic water use by type sectors[sectors,water](Time\
		), Real total output by sector BGR[scenarios
	,sectors]), 0)
	~	dam3/Mdollars
	~		|

Total water use by type[scenarios, water]=
	SUM(Water use by sector[scenarios,sectors!,water])+Water use by households[scenarios\
		,water]+"Total water for O&M required by RES elec dam3"[scenarios,water]
	~	dam3
	~	Total water consumption by type (green, blue, grey).
	|

"Total water for O&M required by RES elec dam3"[scenarios,water]=
	"Total water for O&M required by RES elec"[scenarios, water]*Mt to dam3
	~	dam3
	~		|

Water intensity by sector[scenarios,sectors,water]= INTEG (
	Variation water intensity by sector[scenarios,sectors,water],
		Initial water intensity by sector[scenarios,sectors,water])
	~	dam3/$1995
	~		|

water:
	blue water, green water, gray water
	~	dam3
	~	Types of water considered in MEDEAS (from WIOD environmnetal satellite \
		accounts).
	|

Water use by sector[scenarios,sectors,water]=
	Water intensity by sector[scenarios,sectors,water]*Real total output by sector BGR[scenarios\
		,sectors]
	~	dam3
	~	Water use by type by sector.
	|

Total CO2 emissions GTCO2[scenarios]=
	CO2 fossil fuel emissions[scenarios]+"CO2 soil&LUCF emissions"[scenarios]+BioE CO2 emissions\
		[scenarios]+CO2 emissions peat-Afforestation program 2020 GtCO2
	[scenarios]
	~	GtCO2/Year
	~	Total annual CO2 emissions. Original unit: "mill Tn CO2"
	|

PE traditional biomass EJ delayed 1yr[scenarios]=
	DELAY FIXED(PE traditional biomass consum EJ[scenarios], TIME STEP, 0)
	~	EJ/Year
	~	Annual primary energy consumption of traditional biomass. It also includes \
		charcoal and biosolids for solids.
	|

Population dependent on trad biomass[scenarios]=
	ZIDZ( PE traditional biomass consum EJ[scenarios] , PEpc consumption people depending on trad biomass\
		)
	~	people
	~	Population dependent on traditional biomass.
	|

Mt to dam3=
	GET XLS CONSTANTS('inputs_BGR.xlsx', 'Constants', 'G17')
	~	Dmnl
	~		|

