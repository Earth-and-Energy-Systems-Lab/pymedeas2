extraction uranium EJ[scenarios]:INTERPOLATE:
	~	
	~	Global uranium extracted. Source: global model.
	|

extraction coal EJ[scenarios]:INTERPOLATE:
	~	
	~	Global primary energy supply. Source: global model.
	|

"PES coal for Heat-nc plants"[scenarios]=
	(extraction coal EJ BGR[scenarios]+imports BGR coal from RoW EJ[scenarios])*"share coal dem for Heat-nc"\
		[scenarios]
	~	EJ
	~	Primary energy supply of coal for non-commercial Heat plants.
	|

PE losses coal for Elec EJ[scenarios]=
	(extraction coal EJ BGR[scenarios]+imports BGR coal from RoW EJ[scenarios])*share coal dem for Elec\
		[scenarios]*(1-efficiency coal for electricity)
	~	EJ/Year
	~	(Primary) Energy losses in the generation of electricity in coal power \
		centrals.
	|

abundance coal BGR[scenarios]=
	IF THEN ELSE((extraction coal EJ BGR[scenarios]+imports BGR coal from RoW EJ[scenarios\
		])>PED coal EJ[scenarios], 1, 1-ZIDZ( (PED coal EJ[scenarios]-extraction coal EJ BGR
	[scenarios]-imports BGR coal from RoW EJ[scenarios]), PED coal EJ[scenarios]))
	~	Dmnl
	~	The parameter abundance varies between (1;0). Abundance=1 while the supply \
		covers the demand; the closest to 0 indicates a higher divergence between \
		supply and demand.
	|

FE Elec generation from coal TWh[scenarios]=
	(extraction coal EJ BGR[scenarios]+imports BGR coal from RoW EJ[scenarios])*efficiency coal for electricity\
		*share coal dem for Elec[scenarios]/EJ per TWh
	~	TWh/Year
	~	Final energy electricity generation from coal (TWh).
	|

"PES coal for Heat-com plants"[scenarios]=
	(extraction coal EJ BGR[scenarios]+imports BGR coal from RoW EJ[scenarios])*"share coal dem for Heat-com"\
		[scenarios]
	~	EJ
	~	Primary energy supply of coal for commercial Heat plants.
	|

PED BGR coal from RoW[scenarios]=
	MAX(0, PED coal EJ[scenarios]-extraction coal EJ BGR[scenarios])
	~	EJ
	~		|

"Share E-losses CC"[scenarios]:INTERPOLATE:
	~	
	~	Energy losses due to climate change impacts. Source: global model.
	|

FE nuclear Elec generation TWh[scenarios]=
	(extraction uranium EJ BGR[scenarios]+imports uranium BGR from RoW[scenarios])*efficiency uranium for electricity\
		/EJ per TWh
	~	TWh/Year
	~	Final energy electricity generation from uranium (TWh).
	|

effects shortage uranium[scenarios]=
	IF THEN ELSE((extraction uranium EJ BGR[scenarios]+imports uranium BGR from RoW[scenarios\
		])=0, 0,
	IF THEN ELSE(abundance uranium[scenarios]>0.8, ((abundance uranium[scenarios]-0.8)*5\
		)^2, 0))
	~	Dmnl
	~	The eventual scarcity of coal would likely constrain the development of \
		CTL. The proposed relationship avoids an abrupt limitation by introducing \
		a range (1;0.8) in the gas abundance that constrains the development of \
		CTL.
	|

Total consumption NRE EJ[scenarios]=
	extraction coal EJ BGR[scenarios]+imports BGR coal from RoW EJ[scenarios]+real extraction conv gas EJ\
		[scenarios]+real extraction conv oil EJ
	[scenarios]
	+real extraction unconv gas EJ[scenarios]+real extraction unconv oil EJ[scenarios]+extraction uranium EJ BGR\
		[scenarios]+"imports BGR nat. gas from RoW EJ"
	[scenarios]+imports BGR total oil from RoW EJ[scenarios]+imports uranium BGR from RoW\
		[scenarios]
	~	EJ/Year
	~	Annual total consumption of non-renewable energy resources.
	|

CC impacts feedback shortage coeff[scenarios]=
	(1-"Share E-losses CC World"[scenarios])
	~	Dmnl
	~	This coefficient adapts the real final energy by fuel to be used by \
		economic sectors taking into account climate change impacts.
	|

PE losses uranium for Elec EJ[scenarios]=
	(extraction uranium EJ BGR[scenarios]+imports uranium BGR from RoW[scenarios])*(1-efficiency uranium for electricity\
		)
	~	EJ/Year
	~	(Primary) Energy losses in the generation of electricity in nuclear power \
		centrals.
	|

"Share E-losses CC World"[scenarios]=
	IF THEN ELSE("activate ELF by scen?"[scenarios]=1,"Share E-losses CC"[scenarios],0)
	~	
	~		|

Total FE Heat consumption EJ[scenarios]=
	Total FE Heat generation EJ[scenarios]/(1+Share heat distribution losses)
	~	EJ
	~	Total final heat consumption (fossil fuels, nuclear, waste & renewables) \
		(EJ).
	|

imports demand BGR uranium from RoW EJ[scenarios]=
	PE demand uranium BGR EJ[scenarios]-extraction uranium EJ BGR[scenarios]
	~	EJ
	~		|

abundance uranium[scenarios]=
	IF THEN ELSE(PE demand uranium BGR EJ[scenarios]=0, 1, IF THEN ELSE((extraction uranium EJ BGR\
		[scenarios]+imports uranium BGR from RoW[scenarios])>PE demand uranium BGR EJ
	[scenarios], 1, 1-((PE demand uranium BGR EJ[scenarios]-extraction uranium EJ BGR[scenarios\
		]-imports uranium BGR from RoW[scenarios])/PE demand uranium BGR EJ[scenarios]))
	)
	~	Dmnl
	~	The parameter abundance varies between (1;0). Abundance=1 while the supply \
		covers the demand; the closest to 0 indicates a higher divergence between \
		supply and demand.
	|

abundance liquids[scenarios]=
	IF THEN ELSE(PED liquids EJ[scenarios]<PES Liquids EJ[scenarios], 1, 1- ZIDZ( PED liquids EJ\
		[scenarios
	]-PES Liquids EJ[
	scenarios] , PED liquids EJ[scenarios]))
	~	Dmnl
	~	The parameter abundance varies between (1;0). Abundance=1 while the supply \
		covers the demand; the closest to 0 indicates a higher divergence between \
		supply and demand.
	|

PE losses oil for Elec EJ[scenarios]=
	(PES total oil EJ BGR[scenarios]+imports BGR total oil from RoW EJ[scenarios])*share oil dem for Elec\
		[scenarios]*(1-efficiency liquids for electricity)
	~	EJ/Year
	~	Primary energy losses related with oil for electricity generation.
	|

PE losses uncon gas for Elec EJ[scenarios]=
	(real extraction unconv gas EJ[scenarios]+imports BGR unconv gas from RoW EJ[scenarios\
		])*"share nat. gas dem for Elec"[scenarios]*(1-efficiency gas for electricity)
	~	EJ/Year
	~	(Primary) Energy losses in the generation of electricity in gas power \
