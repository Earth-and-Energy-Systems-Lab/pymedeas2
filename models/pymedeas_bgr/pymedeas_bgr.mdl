P share oil oil Elec[scenarios]=
	MAX(0, "a lineal regr phase-out oil for elec"[scenarios]*Time+"b lineal regr phase-out FF for elec"\
		[scenarios])
	~	Dmnl
	~	Share oil for electricity generation derived from the phase-out policy.
	|

"a lineal regr phase-out oil for elec"[scenarios]=
	(share in target year FF for elec[scenarios]-"Hist share oil/FF Elec")/("target year policy phase-out FF for elec"\
		[scenarios]-"start year policy phase-out oil for elec"[scenarios])
	~	
	~	a parameter of lineal regression "y=a*TIME+b" where y corresponds to the \
		evolution of the share of oil for electricity over time.
	|

share oil for Elec[scenarios]=
	IF THEN ELSE("switch scarcity-PS elec substit"=0, "Hist share oil/FF Elec",
	IF THEN ELSE(Time<2016, "Hist share oil/FF Elec", 
	IF THEN ELSE("phase-out oil for electricity?"[scenarios]=0, "Future share oil/FF for Elec"\
		[scenarios], 
	IF THEN ELSE(Time<"start year policy phase-out oil for elec"[scenarios], "Hist share oil/FF Elec"\
		, P share oil oil Elec[scenarios]))))
	~	Dmnl
	~	Oil share of electricity demand.
	|

"b lineal regr phase-out FF for elec"[scenarios]=
	share in target year FF for elec[scenarios]-"a lineal regr phase-out oil for elec"[scenarios\
		]*"target year policy phase-out FF for elec"[scenarios]
	~	
	~	b parameter of lineal regression "y=a*TIME+b" where y corresponds to the \
		evolution of the share of oil for electricity over time.
	|

