replacement capacity RES elec[RES elec, scenarios]=
	IF THEN ELSE(Time<2015,0,replacement rate RES elec[RES elec,scenarios]*wear RES elec\
		[RES elec,scenarios]*(1-RES elec tot overcapacity
	[scenarios]))*constraint elec storage availability[scenarios,RES elec]*shortage BioE for elec\
		[RES elec,scenarios]^2
	~	TW/Year
	~	Annual replacement of RES infrastructure for electricity generation by \
		technology. It is assumed that the step of planning the replaced \
		infrastructure can be done while the infraestructure to be replaced is \
		still under operation. For replaced infraestructures, the construction \
		time should be smaller than for new infaestructures, however we compensate \
		for this assuming that the demantling time is included in onstruction time \
		for replaced infrastructure.
	|

adapt growth RES elec after allocation[RES elec, scenarios]=
	IF THEN ELSE(activate EROI allocation rule=0, adapt growth RES elec[RES elec,scenarios\
		], adapt growth RES elec[RES elec
	,scenarios]*EROI allocation rule per RES elec[RES elec,scenarios])*constraint elec storage availability\
		[scenarios,RES elec]
	~	Dmnl
	~	Annual growth per RES elec technology after accounting for the allocation \
		rule.
	|

"max additional potential land for biofuels (compet)"[BAU]=
	GET XLS CONSTANTS('inputs_BGR.xlsx', 'BAU', 'C42') ~~|
"max additional potential land for biofuels (compet)"[SCEN1]=
	GET XLS CONSTANTS('inputs_BGR.xlsx', 'SCEN1', 'C42') ~~|
"max additional potential land for biofuels (compet)"[SCEN2]=
	GET XLS CONSTANTS('inputs_BGR.xlsx', 'SCEN2', 'C42') ~~|
"max additional potential land for biofuels (compet)"[SCEN3]=
	GET XLS CONSTANTS('inputs_BGR.xlsx', 'SCEN3', 'C42') ~~|
"max additional potential land for biofuels (compet)"[SCEN4]=
	GET XLS CONSTANTS('inputs_BGR.xlsx', 'SCEN4', 'C42') ~~|
"max additional potential land for biofuels (compet)"[User defined]=
	GET XLS CONSTANTS('inputs_BGR.xlsx', 'User defined', 'C42')
	~	MHa
	~	Additional (apart from historically used) available land for biofuels in \
		competition with other uses as a share of total agricultural land.
	|

Additional land compet available for biofuels[scenarios]=
	Agricultural land[scenarios]*"max additional potential land for biofuels (compet)"[scenarios\
		]
	~	MHa
	~	Available land for biofuels in competition with other uses depending on \
		the scenario.
	|

new biofuels 2gen land compet[scenarios]=
	IF THEN ELSE(check liquids[scenarios]<0, "constrain liquids exogenous growth?"[scenarios\
		]*Land compet biofuels 2gen Mha
	[scenarios], MAX(Annual additional historic land use biofuels 2gen[scenarios]+adapt growth biofuels 2gen\
		[scenarios]
	*Land compet biofuels 2gen Mha[scenarios]*Biofuels land compet available[scenarios],\
		0))*"scarcity agricultural land?"[scenarios]
	~	MHa/Year
	~	New land dedicated to biofuels 2nd generation in land competing with other \
		uses.
	|

Deforestation rate[scenarios]=
	IF THEN ELSE( "'Available' forest area"[scenarios]>P minimum forest[BAU], unsustainable loggin\
		[scenarios],0)*aux reach available forest[scenarios]
	~	MHa/Year
	~	Forest land deforestation rate due to unsustainable loggin and converted \
		to agriculture uses.
	|

"'Available' to primary forest rate"[scenarios]=
	IF THEN ELSE(Time<2014, hist variation primary forest , 
	IF THEN ELSE(Time<Start year P variation primary forest[scenarios],  Historic av variation primary forests area\
		*Primary forests area
	[scenarios], P variation primary forest[scenarios]*Primary forests area[scenarios]))\
		*aux reach available forest[scenarios]
	~	MHa/Year
	~	Rate of variation of the area occupied by primary forests.
	|

Forest loss to sustain agriculture[scenarios]=
	IF THEN ELSE(aux reach available land[scenarios]<1, agricultural land until 2015[scenarios\
		]-Agricultural land[scenarios], 0)*aux reach available forest[scenarios]
	~	MHa/Year
	~	Forest loss rate to maintain the area dedicated to agriculture in EU in \
		the year 2015.
	|

aux reach available forest[scenarios]= WITH LOOKUP (
	"'Available' forest area"[scenarios],
		([(-0.01,0)-(100,10)],(-0.01,0),(0,0),(1e-008,0),(0.0001,1),(0.01,1),(1,1),(100,1) \
		))
	~	Dmnl
	~	This variable = 0 when there is no more forest area available.
	|

urban land rate[scenarios]=
	IF THEN ELSE(Time<2014, hist variation urban land, 0.000682185*urban land density variation\
		[scenarios]+9.9595*1e-009*pop variation[scenarios])*aux reach ag land[scenarios]
	~	MHa/Year
	~	Rate of urban surface rate.
	|

aux reach ag land[scenarios]= WITH LOOKUP (
	Agricultural land[scenarios],
		([(-0.01,0)-(100,10)],(-0.01,0),(0,0),(1e-008,0),(0.0001,1),(0.01,1),(1,1),(100,1) \
		))
	~	Dmnl
	~	This variable = 0 when there is no more agriculture land available.
	|

"scarcity agricultural land?"[scenarios]=
	
	IF THEN ELSE(aux reach ag land[scenarios]=0, 0,
	IF THEN ELSE(agricultural land pc[scenarios]>agricultural land pc until 2015[scenarios\
