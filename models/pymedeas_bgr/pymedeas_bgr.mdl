	~	Dmnl
	~		|

Cp EV batteries required[scenarios]=
	MAX(0, demand EV batteries for elec storage[scenarios]/EV batteries TW[scenarios])
	~	TW
	~		|

output EV bateries for storage over lifetime[scenarios]=
	Cp EV batteries for elec storage[scenarios]*"new+replaced batteries TW"[scenarios]*(\
		1/TWe per TWh)*lifetime EV batteries[scenarios]*EJ per TWh
	~	EJ
	~	Total electricity output generated over the full operation of the \
		infrastructure of the new capacity installed.
	|

discarded batteries[scenarios]=
	MAX(0 ,ZIDZ( "batteries EV+hib+2wE"[scenarios] , lifetime EV batteries[scenarios] ))
	~	batteries
	~	Discarded electric batteries due to wear.
	|

ESOI elec storage[scenarios]=
	(ESOI PHS[scenarios]*installed capacity PHS TW[scenarios]+ESOI EV batteries[scenarios\
		]*Used EV batteries for elec storage[scenarios])/Total capacity elec storage TW[scenarios\
		]
	~	Dmnl
	~	ESOI of electric storage (PHS and EV batteries).
	|

ESOI EV batteries[scenarios]=
	lifetime EV batteries[scenarios]*Cp EV batteries for elec storage[scenarios]*MW in 1 year to MJ\
		/("g=quality of electricity"[scenarios]*"Energy intensity construction EV batteries MJ/MW"\
		[scenarios]*(1+Share energy requirements for decom EV batteries+Grid correction factor EV batteries\
		))
	~	Dmnl
	~	ESOI batteries of electric vehicles for electricity storage.
		(To estimate the ESOI static: g=0.7 and constant recycling rates)
	|

rt elec storage efficiency[scenarios]=
	(rt storage efficiency PHS*installed capacity PHS TW[scenarios]+rt storage efficiency EV batteries\
		*Used EV batteries for elec storage[scenarios])/Total capacity elec storage TW[scenarios\
		]
	~	Dmnl
	~	Round-trip storage efficiency of electric storage (PHS and EV batteries).
	|

abundance storage[scenarios]=
	1-IF THEN ELSE(demand storage capacity[scenarios] <= Total capacity elec storage TW[\
		scenarios], 1,
	MAX(0, 1-(demand storage capacity[scenarios]-Total capacity elec storage TW[scenarios\
		])/Total capacity elec storage TW[scenarios]))
	~	Dmnl
	~	Increases the planning of PHS if there is a deficit of electric storage.
	|

Used EV batteries for elec storage[scenarios]=
	EV batteries TW[scenarios]*Cp EV batteries for elec storage[scenarios]
	~	TW
	~	Bateries from electric vehicles used for electric storage.
	|

max Cp EV batteries=
	Net stored energy EV battery over lifetime/(min lifetime EV batteries*MW in 1 year to MJ\
		*(kW per battery EV/kW per MW))
	~	Dmnl
	~	Maximum Cp allowed associated to the minimum lifetime.
	|

Total capacity elec storage TW[scenarios]=
	installed capacity PHS TW[scenarios]+Used EV batteries for elec storage[scenarios]
	~	TW
	~	Total capacity electricity storage installed.
	|

demand EV batteries for elec storage[scenarios]=
	MAX(0, demand storage capacity[scenarios]-installed capacity PHS TW[scenarios])
	~	TW
	~	Demand of EV batteries for storage of electricity.
	|

CED per TW over lifetime PHS[scenarios]=
	ZIDZ( (Cp PHS*lifetime RES elec[hydro]*EJ per TWh/TWe per TWh) , ESOI static PHS[scenarios\
		]*quality of electricity 2015
[scenarios] )
	~	EJ/TW
	~		|

remaining potential constraint on new PHS capacity[scenarios]=
	IF THEN ELSE(remaining potential PHS[scenarios]>threshold remaining potential new capacity\
		,1,remaining potential PHS[scenarios]*(1/threshold remaining potential new capacity\
		))
	~	Dmnl
	~		|

new required PHS capacity[scenarios]=
	IF THEN ELSE(Time<(2014-"total time plan+constr RES elec"[hydro]), Historic new required capacity PHS\
		, installed capacity PHS TW
	[scenarios]*adapt growth PHS[scenarios]*remaining potential constraint on new PHS capacity\
		[scenarios])
	~	TW
	~	IF THEN ELSE(Time<(2014-"total time plan+constr RES elec"[RES elec]), Historic new \
		required capacity RES elec[RES elec],installed capacity RES elec TW[RES \
		elec, scenarios]*adapt growth RES elec after allocation[RES \
		elec,scenarios]*remaining potential constraint on new RES elec \
		capacity[RES elec,scenarios]*abundance RES elec2[scenarios])
		
		0.9*installed capacity PHS TW[scenarios]*(1-(installed capacity PHS \
		TW[scenarios]/demand storage capacity[scenarios]))
	|

share dyn FEI for RES vs TFEC[scenarios]=
	Total dyn FEI RES[scenarios]/Real TFEC[scenarios]
