"Initial energy cons per unit of material cons (recycled)"[materials]=
	IF THEN ELSE("Initial energy cons per unit of material cons (recycled) - data"[materials\
		]=0,"Initial energy cons per unit of material cons (virgin)"[materials], "Initial energy cons per unit of material cons (recycled) - data"\
		[materials])
	~	MJ/kg
	~	Energy consumption required to use recycled materials per unit of material \
		consumption. When data for recycled materials was not available, the \
		energy consumption for virgin materials was assumed.
	|

"materials required for O&M RES elec Mt"[RES elec, materials, scenarios]=
	installed capacity RES elec TW[RES elec,scenarios]*"materials for O&M per capacity installed RES elec"\
		[RES elec,materials]*M per T/kg per Mt
	~	Mt
	~	Annual materials required for the operation and maintenance of the \
		capacity of RES for electricity in operation by technology.
	|

TPES EJ[scenarios]=
	Total consumption NRE EJ[scenarios]+TPE from RES EJ[scenarios]+PES waste EJ[scenarios\
		]
	~	EJ/Year
	~	Total Primary Energy Supply.
	|

"FES heat-com from waste EJ"[scenarios]=
	"FES waste for heat-com plants"[scenarios]+"FES heat-com from waste in CHP plants"[scenarios\
		]
	~	EJ
	~	TFES commercial heat from waste.
	|

"FES waste for heat-com plants"[scenarios]=
	"PES waste for heat-com plants"[scenarios]*efficiency waste for heat plants
	~	EJ
	~	Final energy supply of heat in commercial Heat plants from waste.
	|

FES elec from waste in CHP plants[scenarios]=
	PES waste for CHP plants[scenarios]*efficiency waste for elec CHP plants
	~	EJ
	~	Final energy supply of elec in CHP plants from waste.
	|

FES elec from waste in elec plants[scenarios]=
	PES waste for elec plants[scenarios]*efficiency waste for elec plants
	~	EJ
	~	Final energy supply of electricity in Elec plants from waste.
	|

"FES heat-com from waste in CHP plants"[scenarios]=
	PES waste for CHP plants[scenarios]*efficiency waste for heat CHP plants
	~	EJ
	~	Final energy supply of commercial heat in CHP plants from waste.
	|

"PES waste for heat-com plants"[scenarios]=
	PES waste EJ[scenarios]*"share PES waste for heat-com plants"
	~	EJ
	~	Primary energy supply of commercial heat in Heat plants from waste.
	|

"share PES waste for heat-com plants"=
	GET XLS CONSTANTS('inputs_BGR.xlsx' , 'Parameters', 'C79')
	~	Dmnl
	~	Share of PES waste for commercial heat plants.
	|

share PES waste TFC=
	GET XLS CONSTANTS('inputs_BGR.xlsx' , 'Parameters', 'C82')
	~	Dmnl
	~	Share of PES waste for total final consumption.
	|

share PES waste for CHP=
	GET XLS CONSTANTS('inputs_BGR.xlsx' , 'Parameters', 'C81')
	~	Dmnl
	~	Share of PES waste for CHP plants.
	|

share PES waste for elec plants=
	GET XLS CONSTANTS('inputs_BGR.xlsx' , 'Parameters', 'C80')
	~	Dmnl
	~	Share of PES waste for elec plants.
	|

new waste supply EJ[scenarios]=
	IF THEN ELSE(Time<2014, (Historic PES waste EJ(Time+1)-Historic PES waste EJ(Time)),\
		 
	IF THEN ELSE(max PE waste[scenarios]=0,PES waste EJ[scenarios]*P waste change[scenarios\
		],
	((max PE waste[scenarios]-PES waste EJ[scenarios])/max PE waste[scenarios])*adapt growth waste\
		[scenarios]*PES waste EJ[scenarios]))
	~	EJ/Year
	~	New annual waste primary energy supply.
	|

waste change[scenarios]=
	IF THEN ELSE(Annual GDP growth rate BGR[scenarios]<0, Annual GDP growth rate BGR[scenarios\
		], P waste change[scenarios])
	~	1/Year
	~	If GDP becomes negative, annual PES change follows it decreasing trends.
	|

FES elec from waste EJ[scenarios]=
	FES elec from waste in CHP plants[scenarios]+FES elec from waste in elec plants[scenarios\
		]
	~	EJ
	~	TFES electricity from waste.
	|

FES elec from waste TWh[scenarios]=
	FES elec from waste EJ[scenarios]/EJ per TWh
	~	TWh
	~	TFES electricity from waste.
	|

efficiency waste for elec plants=
	GET XLS CONSTANTS('inputs_BGR.xlsx' , 'Parameters', 'C84')
	~	Dmnl
	~	Efficiency of the transformation of waste in elec plants.
	|

PES waste EJ[scenarios]= INTEG (
	new waste supply EJ[scenarios],
		initial PES waste)
	~	EJ
	~	Waste primary energy supply (includes industrial and municipal (renew and \
		non-renew).
	|

PES waste for CHP plants[scenarios]=
	PES waste EJ[scenarios]*share PES waste for CHP
	~	EJ
	~	Primary energy supply waste for CHP plants.
	|

efficiency waste for elec CHP plants=
	GET XLS CONSTANTS('inputs_BGR.xlsx' , 'Parameters', 'C86')
	~	Dmnl
	~	Efficiency of the transformation of waste in elec in CHP plants.
	|

